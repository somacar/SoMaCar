# loopback
auto lo
# ethernet
iface lo inet loopback
iface eth0 inet dhcp
# wifi (dhcp config)
auto wlan0
allow-hotplug wlan0
iface wlan0 inet dhcp
wpa-conf /etc/wpa.config
